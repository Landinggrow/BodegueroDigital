---
import siteData from '../../site.json';

const { class: customClass = "" } = Astro.props;
---

<a href={ siteData.nav.button1.link}  target="_blank" rel="noopener noreferrer"
                   class={`text-white bg-[#4673A6] hover:bg-[#7A9EC6]  font-medium rounded-full text-[20px] px-14 py-2.5 sm:px-20 text-center transition-all ${customClass}`}>
                   { siteData.nav.button1.text} 
</a>
