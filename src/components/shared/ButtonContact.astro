---
import siteData from '../../site.json';

const { class: customClass = "" } = Astro.props;
---

<a href={ siteData.nav.button3.link}  target="_blank" rel="noopener noreferrer"
                   class={`text-white bg-[#0078D4] hover:bg-[#005597]  font-medium rounded-full text-[20px] px-14 py-2.5 sm:px-20 text-center transition-all ${customClass}`}>
                   { siteData.nav.button3.text} 
</a>
